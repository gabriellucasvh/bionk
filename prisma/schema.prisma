generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             String    @id @default(uuid())
  name           String?
  username       String?    
  email          String    @unique
  bio            String?
  provider       String?
  emailVefied    DateTime?
  image          String?
  hashedPassword String?
  createdAt      DateTime  @default(now())
}
